<Window x:Class="HashWFT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HashWFT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Addbtn" Content="Add" HorizontalAlignment="Left" Height="88" Margin="46,93,0,0" VerticalAlignment="Top" Width="330" FontSize="24" Click="ke_Click"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="39" Margin="400,142,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="299" FontSize="24"/>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="39" Margin="400,93,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="299" FontSize="24"/>

    </Grid>
</Window>
